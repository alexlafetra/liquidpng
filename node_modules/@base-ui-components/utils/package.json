{
  "name": "@base-ui-components/utils",
  "version": "0.1.0",
  "description": "A collection of React utility functions for Base UI.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/base-ui.git",
    "directory": "packages/utils"
  },
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@floating-ui/utils": "^0.2.10",
    "reselect": "^5.1.1",
    "use-sync-external-store": "^1.5.0"
  },
  "peerDependencies": {
    "@types/react": "^17 || ^18 || ^19",
    "react": "^17 || ^18 || ^19",
    "react-dom": "^17 || ^18 || ^19"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "sideEffects": false,
  "private": false,
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "index",
  "typesVersions": {
    "*": {
      "index": [
        "./cjs/index.d.ts"
      ],
      "*": [
        "./cjs/*/index.d.ts"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "require": {
        "types": "./cjs/*.d.ts",
        "default": "./cjs/*.js"
      },
      "import": {
        "types": "./esm/*.d.ts",
        "default": "./esm/*.js"
      }
    },
    "./store": {
      "require": {
        "types": "./cjs/store/index.d.ts",
        "default": "./cjs/store/index.js"
      },
      "import": {
        "types": "./esm/store/index.d.ts",
        "default": "./esm/store/index.js"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  }
}